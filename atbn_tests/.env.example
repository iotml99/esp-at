# ESP32 AT Command Test Environment Configuration
# Copy this file to .env and fill in your actual values

# WiFi Configuration
WIFI_SSID=YourWiFiSSID
WIFI_PASSWORD=YourWiFiPassword

# Serial Configuration
SERIAL_PORT=COM5
SERIAL_BAUDRATE=115200
SERIAL_TIMEOUT=10

# Test URLs
TEST_HTTP_URL=http://httpbin.org/get
TEST_HTTPS_URL=https://httpbin.org/get
TEST_JSON_URL=https://httpbin.org/json
TEST_POST_URL=https://httpbin.org/post
TEST_LARGE_FILE_URL=https://httpbin.org/bytes/10240
TEST_WEATHER_API_URL=https://api.openweathermap.org/data/2.5/weather?lat=42.567001&lon=1.598100&appid=test123&lang=en&units=metric

# Test File Paths (SD Card)
SD_DOWNLOAD_PATH=/Download
SD_UPLOAD_PATH=/Upload
SD_COOKIES_PATH=/Cookies
SD_RESULTS_PATH=/Upload/results

# Test Configuration
TEST_BASIC_COMMANDS=true
TEST_WIFI_CONNECTION=true
TEST_SD_CARD_OPERATIONS=true
TEST_HTTP_OPERATIONS=true
TEST_ATBN_COMMANDS=true
TEST_VERBOSE_MODE=true
TEST_COOKIE_OPERATIONS=true
TEST_RANGE_DOWNLOADS=true

# Performance Test Settings
PERFORMANCE_TEST_TIMEOUT=800
MAX_DOWNLOAD_TIME=300
MIN_DOWNLOAD_SPEED=0.1

# SSL/TLS Test URLs
SSL_VALID_URL=https://httpbin.org/get
SSL_EXPIRED_URL=https://expired.badssl.com/
SSL_SELF_SIGNED_URL=https://self-signed.badssl.com/
SSL_WRONG_HOST_URL=https://wrong.host.badssl.com/
